@techreport{wang_experimental_2023,
	type = {preprint},
	title = {Experimental estimates of germline mutation rate in eukaryotes: a phylogenetic meta-analysis},
	shorttitle = {Experimental estimates of germline mutation rate in eukaryotes},
	url = {http://biorxiv.org/lookup/doi/10.1101/2023.01.24.525323},
	abstract = {Abstract
          Mutation is the ultimate source of all genetic variation, and over the last ten years the easy availability of whole-genome sequencing has permitted direct estimation of the mutation rate for many non-model species from across the tree of life. In this meta-analysis we make a comprehensive search of the literature for mutation rate estimates in eukaryotes, identifying 138 mutation accumulation (MA) and parent-offspring (PO) sequencing studies covering 70 species. Based on these recently accumulated data, we revisit the mutation rate differences between different phylogenetic lineages and update the known relationships between mutation rate and generation time, population size, and genome size. We do this for single nucleotide mutations (SNM) in all species, and for short indels in the 29 species for which data are available. We do not find a significant difference between MA and PO in estimated mutation rates. However, we confirm that mammal and plant lineages have higher mutation rates than arthropods, and that unicellular eukaryotes have the lowest mutation rates. Generation time and genome size are each positively correlated with mutation rate, even when accounting for phylogenetic relationships. Meanwhile, indel mutation rates are also positively correlated with SNM rates and short deletions are generally more common than short insertions across all the species. However, we note that no estimates of either SNM or indel mutation rates are available for the majority of deeply-branching eukaryotic lineages—or even for most animal phyla. Even among charismatic megafauna, experimental mutation rate estimates remain unknown for reptiles and amphibia and scarce for birds and fish.},
	language = {en},
	urldate = {2023-02-06},
	institution = {Evolutionary Biology},
	author = {Wang, Yiguan and Obbard, Darren J.},
	month = jan,
	year = {2023},
	doi = {10.1101/2023.01.24.525323},
}

@article{machado_pervasive_2020,
	title = {Pervasive {Strong} {Selection} at the {Level} of {Codon} {Usage} {Bias} in \textit{{Drosophila} melanogaster}},
	volume = {214},
	issn = {1943-2631},
	url = {https://academic.oup.com/genetics/article/214/2/511/5930439},
	doi = {10.1534/genetics.119.302542},
	abstract = {Codon usage bias (CUB), where certain codons are used more frequently than expected by chance, is a ubiquitous phenomenon and occurs across the tree of life. The dominant paradigm is that the proportion of preferred codons is set by weak selection. While experimental changes in codon usage have at times shown large phenotypic effects in contrast to this paradigm, genome-wide population genetic estimates have supported the weak selection model. Here we use deep genomic population sequencing of two Drosophila melanogaster populations to measure selection on synonymous sites in a way that allowed us to estimate the prevalence of both weak and strong purifying selection. We ﬁnd that selection in favor of preferred codons ranges from weak ({\textbar}Nes{\textbar}   1) to strong ({\textbar}Nes{\textbar} . 10), with strong selection acting on 10–20\% of synonymous sites in preferred codons. While previous studies indicated that selection at synonymous sites could be strong, this is the ﬁrst study to detect and quantify strong selection speciﬁcally at the level of CUB. Further, we ﬁnd that CUB-associated polymorphism accounts for the majority of strong selection on synonymous sites, with secondary contributions of splicing (selection on alternatively spliced genes, splice junctions, and spliceosome-bound sites) and transcription factor binding. Our ﬁndings support a new model of CUB and indicate that the functional importance of CUB, as well as synonymous sites in general, have been underestimated.},
	language = {en},
	number = {2},
	urldate = {2022-12-20},
	journal = {Genetics},
	author = {Machado, Heather E and Lawrie, David S and Petrov, Dmitri A},
	month = feb,
	year = {2020},
	pages = {511--528},
}

@misc{machado_strong_2017,
	title = {Strong selection at the level of codon usage bias: evidence against the {Li}-{Bulmer} model},
	copyright = {© 2017, Posted by Cold Spring Harbor Laboratory. This pre-print is available under a Creative Commons License (Attribution 4.0 International), CC BY 4.0, as described at http://creativecommons.org/licenses/by/4.0/},
	shorttitle = {Strong selection at the level of codon usage bias},
	url = {https://www.biorxiv.org/content/10.1101/106476v2},
	doi = {10.1101/106476},
	abstract = {1 Abstract
Codon usage bias (CUB), where certain codons are used more frequently than expected by chance, is a ubiquitous phenomenon and occurs across the tree of life. The dominant paradigm is that the proportion of preferred codons is set by weak selection. While experimental changes in codon usage have at times shown large phenotypic effects in contrast to this paradigm, genome-wide population genetic estimates have supported the weak selection model. Here we use deep genomic sequencing of two Drosophila melanogaster populations to measure selection on synonymous sites in a way that allowed us to estimate the prevalence of both weak and strong selection. We find that selection in favor of preferred codons ranges from weak ({\textbar}Nes{\textbar} ∼ 1) to strong ({\textbar}Nes{\textbar} {\textgreater} 10). While previous studies indicated that selection at synonymous sites could be strong, this is the first study to detect and quantify strong selection specifically at the level of CUB. We suggest that the level of CUB in the genome is determined by the proportion of synonymous sites under no, weak, and strong selection. This model challenges the standard Li-Bulmer model and explains some of the longest-standing puzzles in the field.},
	language = {en},
	urldate = {2022-12-20},
	publisher = {bioRxiv},
	author = {Machado, Heather E. and Lawrie, David S. and Petrov, Dmitri A.},
	month = feb,
	year = {2017},
	note = {Pages: 106476
Section: New Results},
}

