@article{ou_errors_2019,
	title = {Errors in translational decoding: {tRNA} wobbling or misincorporation?},
	volume = {15},
	issn = {1553-7404},
	shorttitle = {Errors in translational decoding},
	url = {https://journals.plos.org/plosgenetics/article?id=10.1371/journal.pgen.1008017},
	doi = {10.1371/journal.pgen.1008017},
	abstract = {As the central dogma of molecular biology, genetic information flows from DNA through transcription into RNA followed by translation of the message into protein by transfer RNAs (tRNAs). However, mRNA translation is not always perfect, and errors in the amino acid composition may occur. Mistranslation is generally well tolerated, but once it reaches superphysiological levels, it can give rise to a plethora of diseases. The key causes of mistranslation are errors in translational decoding of the codons in mRNA. Such errors mainly derive from tRNA misdecoding and misacylation, especially when certain codon-paired tRNA species are missing. Substantial progress has recently been made with respect to the mechanistic basis of erroneous mRNA decoding as well as the resulting consequences for physiology and pathology. Here, we aim to review this progress with emphasis on viral evolution and cancer development.},
	language = {en},
	number = {3},
	urldate = {2022-12-12},
	journal = {PLOS Genetics},
